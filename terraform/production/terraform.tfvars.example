# Example terraform.tfvars for production environment
environment            = "production"
aws_region            = "us-west-1"
bucket_name_prefix    = "ysc-media"
# IMPORTANT: Restrict CORS origins to your actual production domains
allowed_cors_origins  = [
  "https://ysc.org"
]

# Lifecycle configuration (only applies to production)
lifecycle_noncurrent_days  = 90
lifecycle_transition_days  = 90
lifecycle_storage_class    = "GLACIER"
